namespace cml.examples

import cml.lang.solidity.*

Integer equation()
	return 8 / 2 * (2 + 2)
    
@FixedPointArithmetic(decimals=2)
contract FixedPointArithmetic
	clause Clause
		party anyone
		may calc
		
	Real calc1()
		return equation() + 2
	
	Real calc2()
		return equation().toReal() + 2.0
		
	Real calc3(Integer a, Real b)
		return a + b